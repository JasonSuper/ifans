<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ifans.system.mapper.UserGoodsBagMapper">

    <select id="selectByUserId" parameterType="java.lang.String" resultType="com.ifans.api.system.domain.UserGoodsBag">
        select * from user_goods_bag
        where user_id = #{userid}
    </select>
</mapper>